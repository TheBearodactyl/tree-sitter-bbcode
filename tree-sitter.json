  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "osu!treesitter",
    "authors": [
      {
        "name": "The Motherfucking Bearodactyl",
        "email": "gannonkoenig@gmail.com"
      }
    ],
    "links": {
